{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch with ts-node",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/index.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register", "-r", "tsconfig-paths/register"],
      "runtimeVersion": "12.14.1",
      "internalConsoleOptions": "openOnSessionStart",
      "sourceMaps": true
    }
  ]
}
